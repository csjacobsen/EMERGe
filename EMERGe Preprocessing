hts_Stats		--stats-file /home/#username/#projectname/output/#projectname.log	 --notes 'compute stats on original dataset' --read1-input /home/#username/#projectname/input/*_R1_* --read2-input /home/#username/#projectname/input/*_R2_* -f /home/#username/#projectname/mid/#PROJECTNAME_Stats
hts_SeqScreener		--append-stats-file /home/#username/#projectname/output/#projectname.log --notes 'remove PhiX' --read1-input /home/#username/#projectname/mid/*Stats_R1.fastq.gz --read2-input /home/#username/#projectname/mid/*Stats_R2.fastq.gz --check-read-2 -f /home/#username/#projectname/mid/#PROJECTNAME_Seq
hts_Overlapper		--append-stats-file /home/#username/#projectname/output/#projectname.log --notes 'Overlap reads' --read1-input /home/#username/#projectname/mid/*Seq_R1.fastq.gz --read2-input /home/#username/#projectname/mid/*Seq_R2.fastq.gz -f /home/#username/#projectname/mid/#PROJECTNAME_Over
hts_Primers		--append-stats-file /home/#username/#projectname/output/#projectname.log --notes 'ID proper amplicons' -U /home/#username/#projectname/mid/*Over*SE* --primers_5p #5primer --primers_3p #3primer --keep --flip -f /home/#username/#projectname/mid/#PROJECTNAME_Primer
hts_NTrimmer            --append-stats-file /home/#username/#projectname/output/#projectname.log --notes 'remove N characters' -U /home/#username/#projectname/mid/*Primer*SE* --exclude -f /home/#username/#projectname/mid/#PROJECTNAME_Trimmer 
hts_LengthFilter        --append-stats-file /home/#username/#projectname/output/#projectname.log --notes 'remove too short/long seqs' -U /home/#username/#projectname/mid/*Trimmer*SE* --min-length 50 --max-length 150 --no-orphans -f /home/#username/#projectname/mid/#PROJECTNAME_Length
hts_Stats		--append-stats-file /home/#username/#projectname/output/#projectname.log --notes 'final stats' -U /home/#username/#projectname/mid/*Length*SE* --force --fastq-output /home/#username/#projectname/output/#PROJECTNAME_preprocesed
